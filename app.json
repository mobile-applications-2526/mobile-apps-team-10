{
  "expo": {
    "name": "Let 'm Cook",
    "slug": "let-m-cook",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "letmcook",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "be.ucll.letmcook"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F5EEE6",
        "foregroundImage": "./assets/images/logo.png"
      },
      "package": "be.ucll.letmcook"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "backgroundColor": "#F5EEE6",
          "resizeMode": "contain",
          "imageWidth": 200,
          "ios": {
            "enableFullScreenImage_legacy": true,
            "resizeMode": "cover"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
